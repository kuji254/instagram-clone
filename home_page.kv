<HomePage>
    md_bg_color: get_color_from_hex("#FFFFFF")
    MDBoxLayout:
        orientation:"vertical"

        AppBar:

        MDBoxLayout:

            ScrollView:
                bar_width:0

                MDBoxLayout:
                    adaptive_height:True

                    MDBoxLayout:
                        orientation:"vertical"
                        adaptive_height:True
                        id:timeline

                        ScrollView:
                            size_hint_y:None
                            height:"110dp"
                            bar_width:0
                            MDBoxLayout:
                                adaptive_width:True
                                spacing:"5dp"
                                padding:"5dp"

                                ##story creator
                                StoryCreator:
                                    avatar:"image_1.jpeg"

                                MDBoxLayout:
                                    adaptive_width:True
                                    padding:"5dp"
                                    spacing:"5dp"
                                    id:stories

                        MDSeparator:
